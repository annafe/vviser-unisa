<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>vviser</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
      <servlet-name>ServletInsertProdottiValutazione</servlet-name>
      <servlet-class>it.unisa.vviser.servlet.ServletInsertProdottiValutazione</servlet-class>
    </servlet>

	<servlet-mapping>
        <servlet-name>ServletInsertProdottiValutazione</servlet-name>
        <url-pattern>/ServletInsertProdottiValutazione</url-pattern>
    </servlet-mapping>
    <servlet>
      <servlet-name>ServletVisualizzaProdotti</servlet-name>
      <servlet-class>it.unisa.vviser.servlet.ServletVisualizzaProdotti</servlet-class>
    </servlet>

	<servlet-mapping>
        <servlet-name>ServletVisualizzaProdotti</servlet-name>
        <url-pattern>/ServletVisualizzaProdotti</url-pattern>
    </servlet-mapping>
    
    <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>it.unisa.vviser.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    
	<servlet>
    <servlet-name>DeleteAccountServlet</servlet-name>
    <servlet-class>it.unisa.vviser.servlet.DeleteAccountServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    <servlet-name>DeleteAccountServlet</servlet-name>
    <url-pattern>/DeleteAccountServlet</url-pattern>
    </servlet-mapping>
</web-app>